{
    "version": 2,
    "builds": [
      {
        "src": "testecadver.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/enviar-dados",
        "methods": ["POST"],
        "dest": "testelistver.js"
      },
      {
        "src": "/(.*)",
        "dest": "testecadver.js"
      }
    ]
  }